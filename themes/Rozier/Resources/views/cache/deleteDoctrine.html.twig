{% extends 'base.html.twig' %}


{% block content %}
<section id="manage-cache">
    <h1 id="group-title">{% trans %}purge.cache{% endtrans %}</h1>

    {% form_theme form 'forms.html.twig' %}
    <form class="uk-form uk-form-stacked" action="#" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <fieldset data-uk-margin>
            <legend>{% trans %}are_you_sure.purge.cache{% endtrans %}</legend>
            <a href="{{ path('adminHomePage') }}" class="uk-button"><i class="uk-icon-reply"></i> {% trans %}cancel{% endtrans %}</a>
            <button class="uk-button uk-button-danger" type="submit"><i class="uk-icon-trash-o"></i> {% trans %}clear{% endtrans %}</button>
        </fieldset>
    </form>
</section>
{% endblock %}
