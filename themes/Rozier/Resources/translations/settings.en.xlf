                <source>openid_hd.help</source>
                <target state="translated">Allows only domain name emails to authenticate against OpenID provider.</target>
                <note>Setting description</note>
            </trans-unit>
      <trans-unit xml:space="preserve" id="openid_button_label">
                <source>openid_button_label</source>
                <target state="translated">Connect with OpenId button label</target>
