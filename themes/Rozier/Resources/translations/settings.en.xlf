                <source>openid_button_label.help</source>
                <target state="translated">Customize login page OpenID button label.</target>
                <note>Setting description</note>
            </trans-unit>
      <trans-unit xml:space="preserve" id="openid_discovery">
                <source>openid_discovery</source>
                <target state="translated">Autodiscovery OpenID URL</target>
                <note>Setting name</note>
            </trans-unit>
