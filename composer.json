    "require": {
        "rezozero/slir": "2.1.*",
        "doctrine/orm": "@stable",
        "symfony/yaml": "@stable",
        "symfony/console": "@stable",
        "symfony/http-foundation": "@stable",
        "symfony/routing": "@stable",
        "symfony/config": "@stable",
        "symfony/http-kernel": "@stable",
        "symfony/stopwatch": "@stable",
        "symfony/form": "@stable",
        "symfony/validator": "@stable",
        "symfony/security-csrf": "@stable",
        "symfony/twig-bridge": "@stable",
        "symfony/security": "@stable",
        "twig/twig":"@stable",
        "guzzlehttp/guzzle":"@stable",
        "swiftmailer/swiftmailer": "@stable",
        "symfony/finder": "@stable",
        "twig/extensions": "@stable",
        "michelf/php-markdown": "@stable",
        "symfony/serializer": "@stable",
        "symfony/filesystem": "@stable",
        "solarium/solarium": "@stable",
        "pimple/pimple": "@stable",
        "inlinestyle/inlinestyle": "@stable"
    },
    "autoload": {
        "psr-4": {
        	"RZ\\": "src/",
            "Themes\\": "themes/",
        	"GeneratedNodeSources\\": "gen-src/GeneratedNodeSources",
            "Proxies\\": "gen-src/Proxies"
        },
        "psr-0": {
            "GlobalUrlMatcher": "gen-src/Compiled/",
            "GlobalUrlGenerator": "gen-src/Compiled/"
        }
    }
}
