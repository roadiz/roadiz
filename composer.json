{
    "name": "rezozero/renzo",
    "version": "0.0.1",
    "require": {
        "rezozero/slir": "2.1.*",
        "doctrine/orm": "2.4.*",
        "symfony/yaml": "2.*",
        "symfony/console": "2.5.0",
        "symfony/http-foundation": "2.5.*",
        "symfony/routing": "2.5.*",
        "symfony/config": "2.5.*",
        "symfony/http-kernel": "2.5.*",
        "symfony/stopwatch": "2.5.*",
        "symfony/form": "2.5.*",
        "symfony/validator": "2.5.*",
        "symfony/security-csrf": "2.5.*",
        "symfony/twig-bridge": "2.5.*",
        "symfony/security": "2.5.*",
        "twig/twig":"1.*",
        "guzzle/http":"3.9.*",
        "swiftmailer/swiftmailer": "@stable",
        "symfony/finder": "@stable",
        "symfony/intl": "@stable",
        "twig/extensions": "@stable",
        "michelf/php-markdown": "@stable",
        "symfony/serializer": "@stable"
    },
    "require-dev":{
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
        	"RZ\\": "src/",
            "Themes\\": "themes/",
        	"GeneratedNodeSources\\": "sources/GeneratedNodeSources",
            "Proxies\\": "sources/Proxies"
        },
        "psr-0": {
            "GlobalUrlMatcher": "sources/Compiled/",
            "GlobalUrlGenerator": "sources/Compiled/"
        }
    }
}
