    "require": {
        "rezozero/slir":           "2.1.*",
        "doctrine/orm":            "2.4.*",
        "symfony/yaml":            "2.5.*",
        "symfony/console":         "2.5.*",
        "symfony/http-foundation": "2.5.*",
        "symfony/routing":         "2.5.*",
        "symfony/config":          "2.5.*",
        "symfony/http-kernel":     "2.5.*",
        "symfony/stopwatch":       "2.5.*",
        "symfony/form":            "2.5.*",
        "symfony/validator":       "2.5.*",
        "symfony/security-csrf":   "2.5.*",
        "symfony/twig-bridge":     "2.5.*",
        "symfony/finder":          "2.5.*",
        "symfony/serializer":      "2.5.*",
        "symfony/filesystem":      "2.5.*",
        "symfony/security":        "2.3.*",
        "twig/twig":               "1.16.*",
        "twig/extensions":         "1.2.*",
        "guzzlehttp/guzzle":       "5.0.*",
        "swiftmailer/swiftmailer": "5.3.*",
        "michelf/php-markdown":    "1.4.*",
        "solarium/solarium":       "3.2.*",
        "pimple/pimple":           "3.0.*",
        "inlinestyle/inlinestyle": "1.2.*",
        "phpoffice/phpexcel":      "1.8.*"
    },
    "autoload": {
        "psr-4": {
        	"RZ\\":                   "src/",
            "Themes\\":               "themes/",
        	"GeneratedNodeSources\\": "gen-src/GeneratedNodeSources",
            "Proxies\\":              "gen-src/Proxies"
        },
        "psr-0": {
            "GlobalUrlMatcher":   "gen-src/Compiled/",
            "GlobalUrlGenerator": "gen-src/Compiled/"
        }
    }
}
